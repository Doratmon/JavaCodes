package com._520it.eclipse;

public class App {
	public static void main(String[] args) {
		System.out.println("begin.........");
		for (int i = 0; 
				i < 10; 
				i++) {
			System.out.println("i = " + i);
			if (i == 5) {
				System.out.println("要退出了");
				break;
			}

		}
		System.out.println("ending........");
	}

}
