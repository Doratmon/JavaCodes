package com._520it._07_regex;

//正则表达式表示表达式正确的规范性
//用途：匹配判断、分割、替换操作
public class RegexDemo {
	public static void main(String[] args) {
		String str = "1234qw56";
		//判断一个验证码是否是全数字
		
		//1.常规方法
		for (int i = 0; i < str.length(); i++) {
			if(str.charAt(i) < '1' || str.charAt(i) > '9'){
				System.out.println("验证码不是全数字");
				break;
			}
			System.out.println("验证码是全数字");
		}
	}

}
